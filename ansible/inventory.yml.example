---
all:
  children:
    nomad_servers:
      hosts:
        nomad-server:
          ansible_host: 192.168.0.200
          consul_server: true
          nomad_server: true

    nomad_clients:
      hosts:
        nomad-client:
          ansible_host: 192.168.0.201
          consul_server: false
          nomad_server: false

    nas:
      hosts:
        openmediavault:
          ansible_host: 192.168.0.202

  vars:
    ansible_user: debian
    ansible_ssh_private_key_file: ~/.ssh/id_rsa
    ansible_python_interpreter: /usr/bin/python3
